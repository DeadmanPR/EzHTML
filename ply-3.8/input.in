TITLE = "MY Website";

body{
	//Headers
	heading1 h1 = "This is a test";
	style(h1){
		color = RED;
		alignment = CENTER;
		font = COURIER;
	};
	heading2 h2 = "Another test";

	//Paragraphs
	paragraph myParagraph = "This is a paragraph!";
	style(myParagraph){
		color = BLUE;
		isBold = TRUE; //Makes text bold
	};

	//HTML Links (Hyperlinks)
	link myLink;
	linkAttr(myLink){
		destination = "http://www.google.com";
		text = "This is Google!"
	};

	//Images
	image testImage;
	imageAttr(testImage){
		source = "http://www.uprm.edu/wdt/resources/portico1.gif";
		width = 100;
		height = 100;
		destination = "http://www.uprm.edu";
	};
};
