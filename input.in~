$$$$
body{
	//Headers
	heading1 h1 = "This is a test";
	style(h1){
		color = RED;
		alignment = CENTER;
		font = COURIER;
	};
	heading2 h2 = "Another test";

	//Paragraphs
	paragraph myParagraph = "This is a paragraph!";
	style(myParagraph){
		color = BLUE;
		isBold = 1; //Makes text bold
	};

	//HTML Links (Hyperlinks)
	link myLink = ("http://www.google.com", "This is Google!");

	//Images
	image testImage = ("http://www.uprm.edu/wdt/resources/portico1.gif",100, 100, "http://www.uprm.edu"); //Source file name (or URL), width, height, link (if any)
};
